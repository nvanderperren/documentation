haObj:hasMaster a rdf:Property ;
    rdfs:label "has archiving master"@en,
        "a copie principale"@fr,
        "heeft archiefmaster"@nl ;
    rdfs:isDefinedBy haObj: ;
    rdfs:subPropertyOf rel:isr ;
