    rdfs:comment "A logo is a a graphic mark, emblem, or symbol of an organization."@en,
        "Een logo is een grafisch embleem of symbool van een organisatie."@nl ;
    rdfs:isDefinedBy haOrg: .

haOrg:OrganizationType a rdfs:Class ;
    rdfs:label "Organization Type"@en,
        "Type de Organisation"@fr,
        "Organisatietype"@nl ;
    rdfs:comment "The type or classification of an organization."@en,
        "Het type of classificatie van een organisatie."@nl ;
    rdfs:isDefinedBy haOrg: ;
    rdfs:subClassOf skos:Concept .

haOrg:School a rdfs:Class ;
    rdfs:label "School"@en,
        "École"@fr,
        "School"@nl ;
    rdfs:comment "Een door de Vlaamse overheid erkende onderwijsinstelling in het basis, secundair or hoger onderwijs."@nl ;
    rdfs:isDefinedBy haOrg: ;
    rdfs:subClassOf haOrg:EducationalOrganization ;
    owl:equivalentClass schema:School .

haOrg:ServiceConsumer a rdfs:Class ;
    rdfs:label "Service Consumer"@en,
        "Consomatteur de Services"@fr,
        "Dienstenafnemer"@nl ;
    rdfs:comment "Een organisatie die diensten bij meemoo afneemt, met contractuele relatie met meemoo. Het gaat hier niet over een contentpartner, maar ever een private organisatie die digitale opslag doet bij meemoo (bv. Plantentuin Meise)."@nl ;
    rdfs:isDefinedBy haOrg: ;
    rdfs:subClassOf org:Organization .

haOrg:ServiceProvider a rdfs:Class ;
    rdfs:label "Fournisseur de Services",
        "Service Provider"@en,
        "Dienstenleverancier"@nl ;
    rdfs:comment "Een organisatie die aan meemoo diensten of goederen levert: kan zowel gaan over IT, digitalisering, ..."@nl ;
    rdfs:isDefinedBy haOrg: ;
    rdfs:subClassOf org:Organization .

schema:ContactPoint a rdfs:Class ;
    rdfs:label "ContactPoint"@en,
        "Point de contact"@fr,
        "Aanspreek-/meldpunt"@nl ;
    rdfs:comment "A contact point&#x2014;for example, a Customer Complaints department."@en,
        "Un point de contact&#x2014;par exemple, un service des plaintes des clients."@fr,
        "Een aanspreekpunt&#x2014;bijvoorbeeld, een afdeling voor klachten van klanten."@nl ;
    rdfs:isDefinedBy schema: ;
    rdfs:subClassOf schema:StructuredValue .

schema:Person a rdfs:Class ;
    rdfs:label "Person"@en,
        "Personne"@fr,
        "Persoon"@nl ;
    rdfs:comment "A person (alive, dead, undead, or fictional)."@en,
        "Une personne (vivante, morte, non-morte ou fictive)."@fr,
        "Een persoon (levend, dood, ondood, of fictief)."@nl ;
