@prefix dc11: <http://purl.org/dc/elements/1.1/> .
@prefix dct: <http://purl.org/dc/terms/> .
@prefix ebucore: <http://www.ebu.ch/metadata/ontologies/ebucore/ebucore#> .
@prefix edm: <http://www.europeana.eu/schemas/edm/> .
@prefix haObj: <https://data.hetarchief.be/ns/object#> .
@prefix ma: <http://www.w3.org/ns/ma-ont#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix pav: <http://purl.org/pav/> .
@prefix premis: <http://www.loc.gov/premis/rdf/v3/> .
@prefix prov: <http://www.w3.org/ns/prov#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rel: <http://id.loc.gov/vocabulary/preservation/relationshipSubType/> .
@prefix schema: <https://schema.org/> .
@prefix seq: <http://www.ontologydesignpatterns.org/cp/owl/sequence.owl#> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix vann: <http://purl.org/vocab/vann/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

dct:FileFormat a rdfs:Class ;
    rdfs:label "File Format"@en,
        "Format de fichier"@fr,
        "Bestandsformaat"@nl ;
    dct:description "Examples include the formats defined by the list of Internet Media Types."@en ;
    dct:hasVersion <http://dublincore.org/usage/terms/history/#FileFormat-001> ;
    dct:issued "2008-01-14"^^xsd:date ;
    rdfs:comment "A digital resource format."@en,
        "Un format de ressource numérique."@fr,
        "Een formaat van een digitale bron."@nl ;
    rdfs:isDefinedBy dct: ;
    rdfs:subClassOf dct:MediaType .

premis:File a rdfs:Class,
        owl:Class ;
    rdfs:label "File"@en,
        "Fichier"@fr,
        "Bestand"@nl ;
    rdfs:comment "Named and ordered sequence of bytes that is known to an operating system."@en,
        "Séquence nommée et ordonnée d'octets/bytes connue par un système d'exploitation."@fr,
        "Genoemde en geordende sequentie van bytes gekend door een besturingssysteem."@nl ;
    rdfs:isDefinedBy premis: ;
    rdfs:subClassOf premis:Object ;
    owl:disjointWith premis:Bitstream,
        premis:IntellectualEntity,
        premis:Representation .

premis:Fixity a rdfs:Class,
        owl:Class ;
    rdfs:label "Fixity"@en,
        "Fixité"@fr,
        "Fixity"@nl ;
    rdfs:comment "Information used to verify whether an object has been altered in an undocumented or unauthorized way."@en,
        "Informations utilisées pour vérifier si un objet a été modifié de manière non documentée ou non autorisée."@fr,
        "Informatie die wordt gebruikt om na te gaan of een object op een ongedocumenteerde of ongeoorloofde manier werd gewijzigd."@nl ;
    rdfs:isDefinedBy premis: ;
    rdfs:seeAlso <http://id.loc.gov/vocabulary/preservation/cryptographicHashFunctions> .

premis:IntellectualEntity a rdfs:Class,
        owl:Class ;
    rdfs:label "Intellectual entity"@en,
        "Entité intellectuelle"@fr,
        "Intellectuele entiteit"@nl ;
    rdfs:comment """A set of content that is considered a single intellectual unit for purposes of management and
      description: for example, a particular book, map, photograph, database, or piece of hardware or
      software. An Intellectual Entity can include other Intellectual Entities; for example, a web site can
      include a web page; a web page can include an image. An Intellectual Entity may have one or more digital
      representations. An Intellectual Entity may also describe an environment, defined as technology
      supporting a digital object in some way (e.g. by rendering or executing it). Environments can consist of
      software, hardware, or a combination of both."""@en,
        "Ensemble de contenu qui est considéré comme une unité intellectuelle pour des objectifs de gestion et de description"@fr,
        "Set van inhoud die beschouwd wordt als één enkele intellectuele eenheid met als doeleinden beheer en beschrijving."@nl ;
    rdfs:isDefinedBy premis: ;
    rdfs:seeAlso <http://id.loc.gov/vocabulary/preservation/environmentFunctionType> ;
    rdfs:subClassOf premis:Object ;
    owl:disjointWith premis:Bitstream,
        premis:File,
        premis:Representation .

premis:Object a rdfs:Class,
        owl:Class ;
    rdfs:label "Object"@en,
        "Objet"@fr,
        "Object"@nl ;
    rdfs:comment "Discrete unit of information subject to digital preservation. Subclasses of Object are Intellectual Entity, Representation, File and Bitstream."@en,
        "Unité discrète d'information soumise à la préservation numérique. Les sous-classes d'objet sont le Intellectual Entity (entité intellectuelle), la Representation (représentation), le File (fichier) et le Bitstream (flux binaire)."@fr,
        "Discrete eenheid van informatie die digitaal moet worden bewaard. Subklassen van Object zijn Intellectual Entity (intellectuele entiteit), Representation (voorstelling/weergave), File (bestand) en Bitstream (bitstream)."@nl ;
    rdfs:isDefinedBy premis: ;
    rdfs:subClassOf prov:Entity .

premis:Representation a rdfs:Class,
        owl:Class ;
    rdfs:label "Representation"@en,
        "Représentation"@fr,
        "Representatie"@nl ;
    rdfs:comment """Digital or physical Object instantiating or embodying an Intellectual Entity. A digital
      representation is the set of stored digital files and structural metadata needed to provide a complete
      and reasonable rendition of the Intellectual Entity. A physical representation is an item such as a
      manuscript, video cassette, or printed document."""@en ;
    rdfs:isDefinedBy premis: ;
    rdfs:subClassOf premis:Object ;
    owl:disjointWith premis:Bitstream,
        premis:File,
        premis:IntellectualEntity .

premis:StorageLocation a rdfs:Class,
        owl:Class ;
    rdfs:label "Storage location"@en,
        "Emplacement de stockage"@fr,
        "Opslagplaats"@nl ;
    rdfs:comment "Information needed to retrieve a physical item from its physical storage location or a file from the storage system, or to access a bitstream within a file."@en,
        "Informations nécessaires pour extraire un élément physique de son emplacement de stockage physique ou un fichier du système de stockage, ou pour accéder à un flux binaire (bitstream) dans un fichier (file)."@fr,
        "Informatie die nodig is om een fysiek voorwerp van zijn fysieke opslagplaats of een bestand uit het opslagsysteem op te halen, of om toegang te krijgen tot een bitstream binnen een bestand (file)."@nl ;
    rdfs:isDefinedBy premis: ;
    rdfs:seeAlso <http://id.loc.gov/vocabulary/preservation/contentLocationType> .

premis:StorageMedium a rdfs:Class,
        owl:Class ;
    rdfs:label "Storage medium"@en,
        "Support de stockage"@fr,
        "Opslagmedium"@nl ;
    rdfs:comment """The physical medium on which the Object is stored (e.g., magnetic tape, hard disk, CD-ROM,
        DVD)."""@en,
        "Le support physique sur lequel l'Objet est stocké (par exemple, le mécanisme magnétique, le disque dur, le CD-ROM, le DVD)."@fr,
        "Het fysieke medium waarop het Object is opgeslagen (bijvoorbeeld, de magnetische tape, de harde schijf, de CD-ROM, de DVD)."@nl ;
    rdfs:isDefinedBy premis: ;
    rdfs:seeAlso <http://id.loc.gov/vocabulary/preservation/storageMedium> .

haObj:DigitalRepresentation a rdfs:Class ;
    rdfs:label "Digital representation"@en,
        "Représentation digitale"@fr,
        "Digitale representatie"@nl ;
    rdfs:comment "Digital representation of an archived intellectual entity, such as an analog AV carrier, a canvas, or a physical object like a sculpture."@en,
        "Représentation digitale d'une entité intellectuelle archivée, par exemple un support analogique de transmission, un canvas, ou un objet physique comme une sculpture."@fr,
        "Digitale representatie van een gearchiveerde intellectuele entiteit, zoals een analoge AV drager, een canvas, of een fysiek object zoals een beeldhouwwerk."@nl ;
    rdfs:isDefinedBy haObj: ;
    rdfs:subClassOf premis:Representation .

haObj:LocalIdentifier a rdfs:Class ;
    rdfs:label "Local identifier"@en,
        "Identifiant local"@fr,
        "Lokale identificatie"@nl ;
    rdfs:comment "A local identifier for an object defined by or known to the content partner, such as a barcode, an identifier from an external database or the identifier in the contentpartner's collection registration system."@en,
        "Un identifiant local pour un objet défini par ou connu par le partenaire de contenu, par exemple un code barre, un identifiant d'un système externe ou l'identifiant dans le système de registre de collection du partenaire de contenu."@fr,
        "Een lokale identificatie voor een object gedefinieerd door of bekend bij de contentpartner, bijvoorbeeld een barcode, een identificatie uit een extern database of de identificatie in het systeem van registratie van de collectie van de partner van content."@nl ;
    rdfs:isDefinedBy haObj: ;
    rdfs:subClassOf premis:Identifier,
        skos:Concept .

haObj:PhysicalRepresentation a rdfs:Class ;
    rdfs:label "Physical representation"@en,
        "Représentation physique"@fr,
        "Fysieke representatie"@nl ;
    rdfs:comment "Physical Representation of an archived intellectual entity, such as an analog AV carrier, a canvas, or a physical object like a sculpture."@en,
        "Représentation physique d'un entité intellectuelle archivée, par exemple un support analogique de transmission, un canvas, ou un objet physique comme une sculpture."@fr,
        "Fysieke representatie van een gearchiveerde intellectuele entiteit, zoals een analoge AV drager, een canvas, of een fysiek object zoals een beeldhouwwerk."@nl ;
    rdfs:isDefinedBy haObj: ;
    rdfs:subClassOf premis:Representation .

rel:gen a owl:ObjectProperty ;
    rdfs:label "generalizes"@en,
        "généralise"@fr,
        "veralgemeent"@nl ;
    rdfs:comment "A relationship between an environment object and another where the described object is a more general description than the more specific related environment. Has a reciprocal relationship of specializes."@en,
        "Une Relation entre un objet d'environnement et un autre objet où l'objet décrit a une description plus générale que l'environnement lié plus spécifique. A une relation réciproque avec specializes."@fr,
        "Een relatie tussen een omgevingsobject en een ander object, waarbij het beschreven object een algemenere beschrijving is dan de meer specifieke verwante omgeving. Heeft een wederkerige relatie met specializes."@nl ;
    rdfs:isDefinedBy <http://id.loc.gov/vocabulary/preservation/relationshipSubType> ;
    rdfs:subPropertyOf premis:relationship .

rel:idc a owl:ObjectProperty ;
    rdfs:label "is Documented In"@en,
        "est documenté dans"@fr,
        "is gedocumenteerd in"@nl ;
    rdfs:comment "A relationship between an environment object and the information that documents it. Has a reciprocal relationship with documents."@en,
        "Une relation entre un objet d'environnement et les informations qui le documentent. A une relation réciproque avec documents."@fr,
        "Een relatie tussen een omgevingsobject en de informatie die het documenteert. Heeft een wederkerige relatie met documents."@nl ;
    rdfs:isDefinedBy <http://id.loc.gov/vocabulary/preservation/relationshipSubType> ;
    rdfs:subPropertyOf premis:relationship .

rel:irq a owl:ObjectProperty ;
    rdfs:label "is Required by"@en,
        "est requis par"@fr,
        "wordt vereist door"@nl ;
    rdfs:comment "The relationship of an environment object to a file or representation, where the environment is required by the other objects to render or run. Has a reciprocal relationship of requires."@en,
        "La relation entre un objet d'environnement et un fichier (file) ou une représentation (representation), lorsque l'environnement est requis par les autres objets pour le rendu ou l'exécution. A une relation réciproque avec requires."@fr,
        "De relatie van een omgevingsobject tot een bestand (file) of representatie (representation), waarbij de omgeving door de andere objecten vereist wordt om te renderen of uit te voeren. Heeft een wederkerige relatie met requires."@nl ;
    rdfs:isDefinedBy <http://id.loc.gov/vocabulary/preservation/relationshipSubType> ;
    rdfs:subPropertyOf premis:relationship .

rel:iso a owl:ObjectProperty ;
    rdfs:label "is Source of"@en,
        "est à la source de"@fr,
        "ligt aan de bron van"@nl ;
    rdfs:comment "The related object is a version of the source object created by a transformation. For example, an HTML file that is the product of a migration from a file encoded in the WordStar format, the Wordstar file has a “isSourceOf” relationship to the HTML file. Also applies to the relationship between the original and a disk image. Has a reciprocal relationship of hasSource."@en,
        "L'objet lié est une version de l'objet source créée par une transformation. Par exemple, un fichier HTML qui est le produit d'une migration à partir d'un fichier codé au format WordStar, le fichier Wordstar a une relation 'isSourceOf' avec le fichier HTML. S'applique également à la relation entre l'original et une image disque. A une relation réciproque avec hasSource."@fr,
        "Het gerelateerde object is een versie van het bronobject gemaakt door een transformatie. Bijvoorbeeld, een HTML-bestand dat het product is van een migratie van een bestand dat is gecodeerd in het WordStar-formaat, het Wordstar-bestand heeft een 'isSourceOf'-relatie met het HTML-bestand. Geldt ook voor de relatie tussen het origineel en een schijfbestand. Heeft een wederkerige relatie met hasSource."@nl ;
    rdfs:isDefinedBy <http://id.loc.gov/vocabulary/preservation/relationshipSubType> ;
    rdfs:subPropertyOf premis:relationship .

rel:iss a owl:ObjectProperty ;
    rdfs:label "is Superseded by"@en,
        "a été remplacé par"@fr,
        "werd vervangen door"@nl ;
    rdfs:comment "A relationship between an environment object and another where the described object has been replaced by another. This allows for an audit trail of environments to be maintained. Has a reciprocal relationship of supersedes."@en,
        "Une relation entre un objet d'environnement et un autre objet où l'objet décrit a été remplacé par un autre. Cela permet de conserver une piste d'audit des environnements. A une relation réciproque avec supersedes."@fr,
        "Een relatie tussen een omgevingsobject en een ander object waarbij het beschreven object is vervangen door een ander. Hierdoor kan een controlespoor van omgevingen worden bijgehouden. Heeft een wederkerige relatie met supersedes."@nl ;
    rdfs:isDefinedBy <http://id.loc.gov/vocabulary/preservation/relationshipSubType> ;
    rdfs:subPropertyOf premis:relationship .

rel:req a owl:ObjectProperty ;
    rdfs:label "requires"@en,
        "nécessite"@fr,
        "vereist"@nl ;
    rdfs:comment "The relationship of an object to another object, where it requires a particular environment or non-environment object to be rendered or to be run. For instance, a file of format Word 97, requires MS Word 1997 to render it. Has a reciprocal relationship of isRequiredBy."@en,
        "La relation d'un objet avec un autre objet, lorsqu'il nécessite un environnement particulier ou un objet non lié à l'environnement pour être rendu ou exécuté comme il faut. Par exemple, un fichier au format Word 97 nécessite MS Word 1997 pour être rendu. A une relation réciproque avec isRequiredBy."@fr,
        "De relatie van een object tot een ander object, waarbij het een bepaald omgevings- of niet-omgevingsobject nodig heeft om te worden weergegeven of om te worden uitgevoerd. Bijvoorbeeld, een bestand met de opmaak Word 97 vereist MS Word 1997 om het weer te geven. Heeft een wederkerige relatie met isRequiredBy."@nl ;
    rdfs:isDefinedBy <http://id.loc.gov/vocabulary/preservation/relationshipSubType> ;
    rdfs:subPropertyOf premis:relationship .

rel:spe a owl:ObjectProperty ;
    rdfs:label "specializes"@en,
        "spécialise"@fr,
        "specialiseert"@nl ;
    rdfs:comment "A relationship between an environment object and another where the described object provides specific characteristics, while the related environment is more generic. Has a reciprocal relationship of generalizes."@en,
        "Une relation entre un objet d'environnement et un autre objet où l'objet décrit fournit des caractéristiques spécifiques, tandis que l'environnement lié est plus générique. A une relation réciproque avec generalizes."@fr,
        "Een relatie tussen een omgevingsobject en een ander object, waarbij het beschreven object specifieke kenmerken vertoont, terwijl de verwante omgeving meer generiek is. Heeft een wederkerige relatie met generalizes."@nl ;
    rdfs:isDefinedBy <http://id.loc.gov/vocabulary/preservation/relationshipSubType> ;
    rdfs:subPropertyOf premis:relationship .

ebucore:hasCaptioning a rdf:Property ;
    rdfs:label "Captioning"@en,
        "Sous-titrage"@fr,
        "Ondertiteling"@nl ;
    rdfs:comment "To signal the presence of Captioning."@en,
        "Pour signaler la présence du sous-titrage."@fr,
        "Om de aanwezigheid van ondertiteling aan te duiden."@nl ;
    rdfs:domain ebucore:Asset ;
    rdfs:isDefinedBy ebucore: ;
    rdfs:range ebucore:Captioning .

ebucore:hasObjectType a rdf:Property ;
    rdfs:label "Object/asset type"@en,
        "Type d'object/actif"@fr,
        "Object-/activatype"@nl ;
    rdfs:comment "To define an ObjectType for the BusinessObject (e.g. book, report, programme, clip) if not defined as a subClass of BusinessObject."@en,
        "Pour définir un ObjectType pour un BusinessObject (par exemple un livre, rapport, programme, clip) s'il n'est pas défini comme une sous-classe de BusinessObject."@fr,
        "Om een ObjectType te definiëren voor een BusinessObject (bv. book, rapport, programma, clip) indien dit niet gedefinieerd is als een subklasse van BusinessObject."@nl ;
    rdfs:domain ebucore:EditorialObject ;
    rdfs:range ebucore:ObjectType,
        xsd:anyURI,
        xsd:string .

premis:size a owl:DatatypeProperty ;
    rdfs:label "has size"@en,
        "a une taille (du fichier)"@fr,
        "heeft (bestands)grootte"@nl ;
    rdfs:domain [ a owl:Class ;
            owl:unionOf ( premis:Bitstream premis:File ) ] ;
    rdfs:isDefinedBy premis: ;
    rdfs:range xsd:nonNegativeInteger ;
    skos:narrowMatch ebucore:fileSize .

seq:directlyFollows a owl:ObjectProperty ;
    rdfs:label "directly follows"@en,
        "suit directement"@fr,
        "volgt direct op"@nl ;
    rdfs:comment "The intransitive follows relation. For example, Thursday directly follows Wednesday. Directness of precedence depends on the designer conceptualization."@en,
        "La relation intransitive de 'suivre directement'. Par exemple, jeudi suit directement mercredi. Le caractère direct de la précédence dépend de la conceptualisation du concepteur."@fr,
        "De intransitieve 'volgt-direct-op' relatie. Bv. donderdag volgt direct op woensdag. De mate van 'directheid' bij het 'volgen-op' hangt af van het ontwerp van de bedenker."@nl ;
    rdfs:domain owl:Thing ;
    rdfs:isDefinedBy <http://www.ontologydesignpatterns.org/cp/owl/sequence.owl> ;
    rdfs:range owl:Thing ;
    rdfs:subPropertyOf seq:follows ;
    owl:inverseOf seq:directlyPrecedes .

<https://data.hetarchief.be/ns/object> a owl:Ontology ;
    dct:author [ schema:email "milan.valadou@meemoo.be" ;
            schema:name "Milan Valadou" ],
        [ schema:email "miel.vandersande@meemoo.be" ;
            schema:name "Miel Vander Sande" ] ;
    dct:created "2022-05-16" ;
    dct:description "Data model to describe objects."@en,
        "Modèle de données pour décrire des objets."@fr,
        "Datamodel voor het beschrijven van objecten."@nl ;
    dct:modified "2022-06-18" ;
    dct:title "Data model Objects"@en,
        "Modèle de données Objets"@fr,
        "Datamodel Objecten"@nl ;
    pav:version "0.0.1" ;
    vann:preferredNamespacePrefix "haObj" ;
    vann:preferredNamespaceUri "http://data.hetarchief.be/ns/object#" ;
    owl:imports rel:,
        premis:,
        prov: .

<https://data.hetarchief.be/ns/object/shapes> dct:author [ schema:email "miel.vandersande@meemoo.be" ;
            schema:name "Miel Vander Sande" ],
        [ schema:email "milan.valadou@meemoo.be" ;
            schema:name "Milan Valadou" ] ;
    dct:created "2022-01-07" ;
    dct:description "SHACL model to validate/describe the structure of achive objects."@en,
        "Modèle SHACL pour valider/décrire des objets."@fr,
        "SHACL-model voor het valideren/beschrijven de structuur van archiefobjecten."@nl ;
    dct:modified "2022-06-16" ;
    dct:title "SHACL Model Objects"@en,
        "Modèle SHACL Objet"@fr,
        "SHACL-model Objecten"@nl .

<https://data.hetarchief.be/ns/object/shapes#DigitalRepresentationShape> a sh:NodeShape ;
    sh:property [ sh:class premis:IntellectualEntity ;
            sh:description "The intellectual entity of which this representation is the archive master."@en,
                "L'entité intellectuelle dont cette représentation est le maître des archives."@fr,
                "De intellectuele entiteit waarvan deze representatie de archiefmaster is."@nl ;
            sh:minCount 1 ;
            sh:path haObj:isMasterOf ;
            sh:severity sh:Violation ],
        [ sh:class premis:IntellectualEntity ;
            sh:description "The intellectual entity of which this digital representation is the mezzanine copy."@en,
                "L'entité intellectuelle dont cette représentation est la copie de mezzanine."@fr,
                "De intellectuele entiteit waarvan deze digitale representatie de mezzanine kopie is."@nl ;
            sh:maxCount 1 ;
            sh:path haObj:isMezzanineOf ;
            sh:severity sh:Violation ],
        [ sh:class premis:IntellectualEntity ;
            sh:description "The intellectual entity of which this digital representation is the access or browse copy."@en,
                "L'entité intellectuelle dont cette représentation digital est l'accès ou la copie de vue."@fr,
                "De intellectuele entiteit waarvan deze digitale representatie de toegangs- of inkijk kopie is."@nl ;
            sh:maxCount 1 ;
            sh:path haObj:isAccesscopyOf ;
            sh:severity sh:Violation ],
        [ sh:class premis:IntellectualEntity ;
            sh:description "The intellectual entity that represents this digital representation."@en,
                "L'entité intellectuelle qui représente cette représentation digital."@fr,
                "De intellectuele entiteit die deze digitale representatie representeert."@nl ;
            sh:minCount 1 ;
            sh:path rel:rep ;
            sh:severity sh:Violation ],
        [ sh:class premis:File ;
            sh:description "A file that is part of this digital representation."@en,
                "Un fichier qui fait partie de cette représentation digital."@fr,
                "Een bestand die deel uit maakt van deze digitale representatie."@nl ;
            sh:minCount 1 ;
            sh:path rel:inc ;
            sh:severity sh:Violation ],
        [ sh:class premis:File ;
            sh:description "The file that must be processed first."@en,
                "Le fichier qui doit être traité en premier."@fr,
                "Het bestand dat eerst verwerkt moet worden."@nl ;
            sh:path rel:hsr ;
            sh:severity sh:Violation ],
        [ sh:class haObj:PhysicalRepresentation ;
            sh:description "The physical carrier from which this digital representation originates from."@en,
                "Le porteur physique dont vient cette représentation numérique."@fr,
                "De fysieke drager waarvan deze digitale representatie afkomstig is."@nl ;
            sh:path rel:hss ;
            sh:severity sh:Violation ],
        [ sh:class premis:File ;
            sh:description "The file of which this is a fragment."@en,
                "Le fichier dont il s'agit d'un fragment."@fr,
                "Het bestand waarvan dit een fragment is."@nl ;
            sh:path ebucore:isMediaFragmentOf ;
            sh:severity sh:Violation ] ;
    sh:targetClass haObj:DigitalRepresentation .

<https://data.hetarchief.be/ns/object/shapes#FileShape> a sh:NodeShape ;
    sh:property [ sh:class haObj:DigitalRepresentation ;
            sh:description "The digital representation this file is part of."@en,
                "La représentation digital dans ce fichier fait partie."@fr,
                "De digitale representatie waarvan dit bestand deel van uitmaakt."@nl ;
            sh:minCount 1 ;
            sh:path rel:isi ;
            sh:severity sh:Violation ],
        [ sh:class premis:File ;
            sh:description "The file that replaces or succeeds this file."@en,
                "Le fichier qui remplace ou succède à ce fichier."@fr,
                "Het bestand dat dit bestand vervangt of opvolgt."@nl ;
            sh:path rel:sup ;
            sh:severity sh:Violation ],
        [ sh:class premis:Fixity ;
            sh:description "The calculated fixity checksum of the file."@en,
                "La somme de contrôle de fixité calculée du fichier."@fr,
                "De berekende fixity checksum van het bestand."@nl ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:path premis:fixity ;
            sh:severity sh:Violation ],
        [ sh:class premis:StorageLocation ;
            sh:description "The location where the file is stored."@en,
                "L'emplacement où le fichier est stocké."@fr,
                "De locatie waar het bestand is opgeslagen."@nl ;
            sh:minCount 1 ;
            sh:path premis:storedAt ;
            sh:severity sh:Violation ],
        [ sh:class dct:FileFormat ;
