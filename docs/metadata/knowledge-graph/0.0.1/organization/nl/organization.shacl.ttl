haOrg:EducationalOrganization a rdfs:Class ;
    rdfs:label "Educational Organization"@en,
        "Organisation Éducative"@fr,
        "Educatieve organisatie"@nl ;
    rdfs:comment "An educational organization is an organization or company that provides educational services outside the school context."@en,
        "Une organisation éducative est une organisation ou une entreprise qui fournit des services éducatifs hors du contexte de l'école."@fr,
        "Een educatieve organisatie is een organisatie of een bedrijf dat educatie en opleiding aanbiedt buiten een schoolcontext."@nl ;
    rdfs:isDefinedBy haOrg: ;
    rdfs:subClassOf org:Organization ;
    owl:equivalentClass schema:EducationalOrganization .

haOrg:EducationalPartner a rdfs:Class ;
    rdfs:label "Educational Partner"@en,
