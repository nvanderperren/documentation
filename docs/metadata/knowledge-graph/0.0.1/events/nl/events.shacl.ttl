@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix schema: <https://schema.org/> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix vann: <http://purl.org/vocab/vann/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

_:ne05af665f42843a7934881a3fb36506db11 a rdfs:Class,
        owl:Class ;
    owl:unionOf ( premis:RightsStatus premis:Rule ) .

_:ne05af665f42843a7934881a3fb36506db14 a rdfs:Class,
        owl:Class ;
    owl:unionOf ( premis:RightsStatus premis:Rule ) .

premis:Agent a rdfs:Class,
        owl:Class ;
    rdfs:label "Agent"@en,
        "Acteur"@fr,
        "Agent"@nl ;
    rdfs:isDefinedBy premis: ;
    rdfs:subClassOf prov:Agent,
        foaf:Agent ;
    skos:definition "Actor (human, machine, or software) associated with one or more Event and/or Rights statement associated with a digital object."@en,
        "Acteur (humain, machine ou logiciel) associé à un ou plusieurs Events et/ou déclarations de Rights associés à un objet numérique/digital."@fr,
        "Agens (mens, machine of software) geassocieerd met één of meer Events en/of Rights uitdrukkingen geassocieerd met een digitaal object."@nl .

premis:Event a rdfs:Class,
        owl:Class ;
    rdfs:label "Event"@en,
        "Événement"@fr,
        "Gebeurtenis"@nl ;
    rdfs:isDefinedBy premis: ;
    rdfs:seeAlso evtType: ;
    rdfs:subClassOf prov:Activity ;
    skos:definition "Action performed within or outside the repository that affects its capability to preserve Objects over the long term."@en,
        "Action réalisée à l'intérieur ou à l'extérieur du dépôt qui affecte sa capacité à préserver des objets sur le long terme."@fr,
        "Handeling uitgevoerd binnen of buiten het archief die het vermogen beïnvloedt om objecten op lange termijn te bewaren."@nl .

premis:HardwareAgent a rdfs:Class,
        owl:Class ;
    rdfs:label "Hardware agent"@en,
        "Matériel acteur"@fr,
        "Hardware agent"@nl ;
    rdfs:isDefinedBy premis: ;
    rdfs:subClassOf premis:Agent ;
    owl:disjointWith premis:Organization,
        premis:Person,
        premis:SoftwareAgent ;
    owl:sameAs <http://id.loc.gov/vocabulary/preservation/agentType/har> ;
    skos:definition "Hardware agent (e.g., a computer) that is associated with one or more Event and/or Rights statement associated with a digital object."@en,
        "Matériel acteur (e.g., un ordinateur) associé à un ou plusieurs Events et/ou déclarations de Rights associés à un objet numérique/digital."@fr,
        "Hardwareagent (bijv. Een computer) dat is gekoppeld aan een of meer evenement- en/of rechtenverklaring die aan een digitaal object is gekoppeld."@nl .

premis:OutcomeStatus a rdfs:Class,
        owl:Class ;
    rdfs:label "Outcome status"@en,
        "Statut de résultat"@fr,
        "Uitkomststatus"@nl ;
    rdfs:isDefinedBy premis: ;
    rdfs:seeAlso <http://id.loc.gov/vocabulary/preservation/eventOutcome> ;
    skos:definition "Overall result of the Event in terms of success, partial success, or failure."@en,
        "Résultat global de l'événement en termes de succès, de succès partiel ou d'échec."@fr,
        "Algemeen resultaat van het Event in termen van succes, gedeeltelijk succes of falen."@nl .

premis:SoftwareAgent a rdfs:Class,
        owl:Class ;
    rdfs:label "Software agent"@en,
        "Logiciel acteur"@fr,
        "Software agent"@nl ;
    rdfs:isDefinedBy premis: ;
    rdfs:subClassOf premis:Agent,
        prov:SoftwareAgent ;
    owl:disjointWith premis:HardwareAgent,
        premis:Organization,
        premis:Person ;
    skos:definition "Software agent (e.g., a computer program) that is associated with one or more Event and/or Rights statement associated with a digital object."@en,
        "Logiciel acteur (e.g., un programme informatique) associé à un ou plusieurs Events et/ou déclarations de Rights associés à un objet numérique/digital."@fr,
        "Softwareagent (bijv. Een computerprogramma) dat is gekoppeld aan een of meer evenement- en/of rechtenverklaring die aan een digitaal object is gekoppeld."@nl .

prov:Activity a rdfs:Class,
        owl:Class ;
    rdfs:label "Activity"@en,
        "Activité"@fr,
        "Activiteit"@nl ;
    rdfs:isDefinedBy <http://www.w3.org/ns/prov-o#> ;
    owl:disjointWith prov:Entity ;
    skos:definition "An activity is something that occurs over a period of time and acts upon or with entities; it may include consuming, processing, transforming, modifying, relocating, using, or generating entities."@en,
        "Une activité est quelque chose qui se passe pendant un certain temps et qui agit sur ou avec des entités ; elle peut inclure la consommation, le traitement, la transformation, la modification, le déplacement, l'utilisation ou la génération des entités."@fr,
        "Een activiteit is iets dat zich gedurende een bepaalde periode voordoet en op of met entiteiten handelt;Het kan onder meer consumeren, verwerken, transformeren, wijzigen, verplaatsen, gebruiken of genereren van entiteiten."@nl .

prov:Agent a rdfs:Class,
        owl:Class ;
    rdfs:label "Agent"@en,
        "Agent"@fr,
        "Agent"@nl ;
    rdfs:isDefinedBy <http://www.w3.org/ns/prov-o#> ;
    owl:disjointWith prov:InstantaneousEvent ;
    skos:definition "An agent is something that bears some form of responsibility for an activity taking place, for the existence of an entity, or for another agent's activity. "@en,
        "Un agent est quelque chose qui porte une forme de responsabilité pour une activité qui se passe, pour l'existence d'une entité ou pour l'activité d'un autre agent. "@fr,
        "Een agent is iets dat een bepaalde vorm van verantwoordelijkheid voor een activiteit neemt, voor de bestaan van een entiteit of voor de activiteit van een andere agent. "@nl .

prov:Entity a rdfs:Class,
        owl:Class ;
    rdfs:label "Entity"@en,
        "Entité"@fr,
        "Entiteit"@nl ;
    rdfs:isDefinedBy <http://www.w3.org/ns/prov-o#> ;
    owl:disjointWith prov:InstantaneousEvent ;
    skos:definition "An entity is a physical, digital, conceptual, or other kind of thing with some fixed aspects; entities may be real or imaginary. "@en,
        "Une entité est un objet physique, numérique, conceptuel ou autre de type fixe ; les entités peuvent être réelles ou imaginaires."@fr,
        "Een entiteit is een fysiek, digitaal, conceptueel of ander soort dingen met sommige vaste aspecten;Entiteiten kunnen echt of denkbeeldig zijn."@nl .

premis:endDate a owl:DatatypeProperty ;
    rdfs:label "end date"@en,
        "date de fin"@fr,
        "einddatum"@nl ;
    rdfs:domain _:ne05af665f42843a7934881a3fb36506db11 ;
    rdfs:isDefinedBy premis: ;
    rdfs:subPropertyOf dct:date .

premis:startDate a owl:DatatypeProperty ;
    rdfs:label "start date"@en,
        "date de début"@fr,
        "startdatum"@nl ;
    rdfs:domain _:ne05af665f42843a7934881a3fb36506db14 ;
    rdfs:isDefinedBy premis: ;
    rdfs:subPropertyOf dct:date .

prov:generatedAtTime a owl:DatatypeProperty ;
    rdfs:label "generated at time"@en,
        "généré à l'heure"@fr,
        "genereerde op tijdstip"@nl ;
    rdfs:domain prov:Entity ;
    rdfs:isDefinedBy <http://www.w3.org/ns/prov-o#> ;
    rdfs:range xsd:dateTime .

prov:invalidatedAtTime a owl:DatatypeProperty ;
    rdfs:label "invalidated at time"@en,
        "invalidated à l'heure"@fr,
        "ongeldig gemaakt op tijdstip"@nl ;
    rdfs:domain prov:Entity ;
    rdfs:isDefinedBy <http://www.w3.org/ns/prov-o#> ;
    rdfs:range xsd:dateTime ;
    skos:definition "The time at which an entity was invalidated (i.e., no longer usable)."@en,
        "Le moment où une entité a été invalidée (c'est-à-dire plus utilisable)."@fr,
        "Het tijdstip waarop een entiteit ongeldig werd (d.w.z. niet langer bruikbaar)."@nl .

prov:wasAssociatedWith a owl:ObjectProperty ;
    rdfs:label "was associated with"@en,
        "a été associé à"@fr,
        "is geassocieerd met"@nl ;
    rdfs:domain prov:Activity ;
    rdfs:isDefinedBy <http://www.w3.org/ns/prov-o#> ;
    rdfs:range prov:Agent ;
    rdfs:subPropertyOf prov:wasInfluencedBy ;
    owl:propertyChainAxiom ( prov:qualifiedAssociation prov:agent ) ;
    skos:definition "An prov:Agent that had some (unspecified) responsibility for the occurrence of this prov:Activity."@en,
        "Un prov:Agent qui avait une responsabilité (non spécifiée) pour la survenue de cette prov:Activity."@fr,
        "Een prov:Agent die enige (niet -gespecificeerde) verantwoordelijkheid had voor het optreden van deze prov:Activity."@nl .

prov:wasAttributedTo a owl:ObjectProperty ;
    rdfs:label "was attributed to"@en,
        "a été attribué à"@fr,
        "is toegekend aan"@nl ;
    rdfs:domain prov:Entity ;
    rdfs:isDefinedBy <http://www.w3.org/ns/prov-o#> ;
    rdfs:range prov:Agent ;
    rdfs:subPropertyOf prov:wasInfluencedBy ;
    owl:propertyChainAxiom ( prov:qualifiedAttribution prov:agent ) ;
    skos:definition "Attribution is the ascribing of an entity to an agent."@en,
        "L'attribution est l'attribution d'une entité à un agent."@fr,
        "Toekenning is het toeschrijven van een entiteit aan een agent."@nl .

<https://data.hetarchief.be/ns/event> a owl:Ontology ;
    dct:author [ schema:email "milan.valadou@meemoo.be" ;
            schema:name "Milan Valadou" ],
        [ schema:email "miel.vandersande@meemoo.be" ;
            schema:name "Miel Vander Sande" ] ;
    dct:created "2022-06-08" ;
    dct:description "Data model to describe events."@en,
        "Modèle de données pour décrire des événements."@fr,
        "Datamodel voor het beschrijven van gebeurtenissen."@nl ;
    dct:modified "2022-06-08" ;
    dct:title "Data model Events"@en,
        "Modèle de données Events"@fr,
        "Datamodel Events"@nl ;
    pav:version "0.0.1" ;
    vann:preferredNamespacePrefix "haEvt" ;
    vann:preferredNamespaceUri "http://data.hetarchief.be/ns/events/" ;
    owl:imports <http://id.loc.gov/vocabulary/preservation/eventOutcome>,
        <http://id.loc.gov/vocabulary/preservation/eventRelatedAgentRole>,
        <http://id.loc.gov/vocabulary/preservation/eventRelatedObjectRole>,
        evtType:,
        premis: .

haEvent:shapes dct:author [ schema:email "miel.vandersande@meemoo.be" ;
            schema:name "Miel Vander Sande" ],
        [ schema:email "milan.valadou@meemoo.be" ;
            schema:name "Milan Valadou" ] ;
    dct:created "2022-08-02" ;
    dct:description "SHACL model to describe events and data provenance."@en,
        "SHACL-model om gebeurtenissen en data herkomst te beschrijven."@nl ;
    dct:modified "2022-08-02" ;
    dct:title "SHACL Model Events"@en,
        "SHACL Model Events"@nl .

<https://data.hetarchief.be/ns/event/shapes#activityShape> a sh:NodeShape ;
    sh:property [ a sh:PropertyShape ;
            sh:description "The generated object."@en,
                "Het gegenereerde object."@nl ;
            sh:maxCount 1 ;
            sh:minCount 0 ;
            sh:name "has generated"@en,
                "heeft gegenereerd"@nl ;
            sh:nodeKind sh:IRI ;
            sh:path prov:generated ],
        [ a sh:PropertyShape ;
            sh:datatype xsd:dateTime ;
            sh:description "The start date of the activity."@en,
                "De startdatum van de activiteit."@nl ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:name "has start date"@en,
                "heeft startdatum"@nl ;
            sh:path prov:startedAtTime ],
        [ a sh:PropertyShape ;
            sh:datatype xsd:dateTime ;
            sh:description "The end date of the activity."@en,
                "De einddatum van de activiteit."@nl ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:name "has end date"@en,
                "heeft einddatum"@nl ;
            sh:path prov:endedAtTime ],
        [ a sh:PropertyShape ;
            sh:class premis:Object ;
            sh:description "The agent that was attributed to the activity."@en,
                "De agent die toegekend is aan de activiteit."@nl ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:name "was attributed to"@en,
                "was toegekend aan"@nl ;
            sh:or ( [ sh:class schema:Person ] [ sh:class org:Organization ] [ sh:class premis:SoftwareAgent ] [ sh:class premis:HardwareAgent ] ) ;
            sh:path prov:wasAtrributedTo ] ;
    sh:targetClass prov:Activity .

<https://data.hetarchief.be/ns/event/shapes#eventShape> a sh:NodeShape ;
    sh:property [ a sh:PropertyShape ;
            sh:class premis:OutcomeStatus ;
            sh:description "The outcome status of the event."@en,
                "De uitkomststatus van de gebeurtenis."@nl ;
            sh:in ( evtOutcome:fai evtOutcome:suc evtOutcome:war ) ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:name "has outcome"@en,
                "heeft uitkomst"@nl ;
            sh:path premis:outcome ],
        [ a sh:PropertyShape ;
            sh:datatype xsd:string ;
            sh:description "A note about the outcome of the event."@en,
                "Een opmerking over de uitkomst van de gebeurtenis."@nl ;
            sh:maxCount 1 ;
            sh:minCount 0 ;
            sh:name "has outcome note"@en,
                "heeft uitkomst opmerking"@nl ;
            sh:path premis:outcomeNote ],
        [ a sh:PropertyShape ;
            sh:datatype xsd:string ;
            sh:description "A note about the event."@en,
                "Een opmerking over de gebeurtenis."@nl ;
            sh:maxCount 1 ;
            sh:minCount 0 ;
            sh:name "has note"@en,
                "heeft opmerking"@nl ;
            sh:path premis:note ],
        [ a sh:PropertyShape ;
            sh:class premis:Object ;
            sh:description "The object that was generated by the event."@en,
                "Het object dat door de gebeurtenis gegenereerd is."@nl ;
            sh:maxCount 1 ;
            sh:minCount 0 ;
            sh:name "result"@en,
                "resultaat"@nl ;
            sh:path evtObjRole:out ],
        [ a sh:PropertyShape ;
            sh:class premis:Object ;
            sh:description "The object that was used as a source for the event."@en,
                "Het object dat als bron gebruikt is voor de gebeurtenis."@nl ;
            sh:maxCount 1 ;
            sh:minCount 0 ;
            sh:name "has source"@en,
                "heeft bron"@nl ;
            sh:path evtObjRole:sou ],
        [ a sh:PropertyShape ;
            sh:class org:Organization ;
            sh:description "The organization that implemented the event."@en,
                "De organisatie die de gebeurtenis heeft geimplementeerd."@nl ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:name "implemented by"@en,
                "geimplementeerd door"@nl ;
            sh:path evtAgRole:imp ],
        [ a sh:PropertyShape ;
            sh:description "The software that executed the event."@en,
                "De software of hardware die de gebeurtenis heeft uitgevoerd."@nl ;
            sh:maxCount 1 ;
            sh:minCount 0 ;
            sh:name "executed by"@en,
                "uitgevoerd door"@nl ;
            sh:or ( [ sh:class premis:SoftwareAgent ] [ sh:class premis:HardwareAgent ] ) ;
            sh:path evtAgRole:exe ] ;
    sh:targetClass premis:Event .

<https://data.hetarchief.be/ns/event/shapes#objectShape> a sh:NodeShape ;
    sh:property [ a sh:PropertyShape ;
            sh:class premis:Event ;
            sh:description "The time at which the object was completely created and is available for use."@en,
                "De tijd waarop het object is volledig aangemaakt en beschikbaar is voor gebruik."@nl ;
            sh:maxCount 1 ;
            sh:minCount 0 ;
            sh:name "was generated by"@en,
                "is gegenereerd door"@nl ;
            sh:path prov:wasGeneratedBy ] ;
    sh:targetClass premis:Object .

evtObjRole:out a owl:ObjectProperty ;
    rdfs:label "outcome"@en,
        "résultat"@fr,
        "uitkomst"@nl ;
    rdfs:domain premis:Event ;
    rdfs:range premis:Object ;
    rdfs:subPropertyOf prov:generated ;
    skos:definition "The object in relation to the described event is the outcome of that event."@en,
        "L'objet dans la relation à l'événement décrit est le résultat de cet événement."@fr,
        "Het object in de relatie met het geschreven event is het resultaat van dat event."@nl .

evtObjRole:sou a owl:ObjectProperty ;
    rdfs:label "source"@en,
        "source"@fr,
        "bron"@nl ;
    rdfs:domain premis:Event ;
    rdfs:range premis:Object ;
    rdfs:subPropertyOf prov:used ;
    skos:definition "The object in relation to the described event that existed prior to that event and was used by it."@en,
        "L'objet dans la relation à l'événement décrit et qui existait avant cet événement et qui l'utilisait."@fr,
        "Het object in de relatie met het geschreven event dat bestond voor dat event en die het gebruikt."@nl .

premis:outcome a owl:ObjectProperty ;
    rdfs:label "has outcome"@en,
        "a résultat"@fr,
        "heeft resultaat"@nl ;
    rdfs:domain premis:Event ;
    rdfs:isDefinedBy premis: ;
    rdfs:range premis:OutcomeStatus .

premis:outcomeNote a owl:DatatypeProperty ;
    rdfs:label "has outcome note"@en,
        "a note de résultat"@fr,
        "heeft resultaatnota"@nl ;
    rdfs:domain premis:Event ;
    rdfs:isDefinedBy premis: ;
    rdfs:range rdfs:Literal ;
    rdfs:subPropertyOf premis:note .

prov:startedAtTime a owl:DatatypeProperty ;
    rdfs:label "started at time"@en,
        "démarré à l'heure"@fr,
        "gestart op tijdstip"@nl ;
    rdfs:domain prov:Activity ;
    rdfs:isDefinedBy <http://www.w3.org/ns/prov-o#> ;
    rdfs:range xsd:dateTime ;
    skos:definition "The time at which an activity started. See also prov:endedAtTime."@en,
        "Le moment où une activité a commencé.Voir aussi prov:endedAtTime."@fr,
        "Het tijdstip waarop een activiteit begon. Zie ook prov:endedAtTime."@nl .

prov:used a owl:ObjectProperty ;
    rdfs:label "used"@en,
        "a utilisé"@fr,
        "gebruikte"@nl ;
    rdfs:domain prov:Activity ;
    rdfs:isDefinedBy <http://www.w3.org/ns/prov-o#> ;
    rdfs:range prov:Entity ;
    rdfs:subPropertyOf prov:wasInfluencedBy ;
    owl:propertyChainAxiom ( prov:qualifiedUsage prov:entity ) ;
    skos:definition "A prov:Entity that was used by this prov:Activity. For example, :baking prov:used :spoon, :egg, :oven ."@en,
        "Une prov:Entity qui a été utilisée par cette activité. Par exemple , :baking prov:used :cuillère , :œuf , :four."@en,
        "Een prov:Entity die door deze prov:Activity werd gebruikt. Bijvoorbeeld: :bakken prov:used :lepel ,:ei, :oven."@nl .

premis:note a owl:DatatypeProperty ;
    rdfs:label "has note"@en,
        "a note"@fr,
        "heeft nota"@nl ;
    rdfs:isDefinedBy premis: ;
    rdfs:range rdfs:Literal ;
    rdfs:subPropertyOf dct:description .

prov:generated a owl:ObjectProperty ;
    rdfs:label "generated"@en,
        "généré"@fr,
        "genereerde"@nl ;
    rdfs:domain prov:Activity ;
    rdfs:isDefinedBy <http://www.w3.org/ns/prov-o#> ;
    rdfs:range prov:Entity ;
    rdfs:subPropertyOf prov:influenced ;
    owl:inverseOf prov:wasGeneratedBy .

prov:wasGeneratedBy a owl:ObjectProperty ;
    rdfs:label "was generated by"@en,
        "a été généré par"@fr,
        "is gegenereerd door"@nl ;
    rdfs:domain prov:Entity ;
    rdfs:isDefinedBy <http://www.w3.org/ns/prov-o#> ;
    rdfs:range prov:Activity ;
    rdfs:subPropertyOf prov:wasInfluencedBy ;
    owl:propertyChainAxiom ( prov:qualifiedGeneration prov:activity ) .

